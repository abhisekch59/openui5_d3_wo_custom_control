{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.demo.d3",
		"type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
		"description": "{{appDescription}}"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.75.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.unified": {}
			}
		},
		"rootView": {
			"viewName": "com.demo.d3.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.demo.d3.i18n.i18n"
				}
			},
			"defaultModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		}
	}
}